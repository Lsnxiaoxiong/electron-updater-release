{
  "name": "electron-updater-release",
  "version": "0.9.1",
  "main": "main.js",
  "description": "electron-updater release project",
  "author": "YiKu",
  "scripts": {
    "publish": "electron-builder --publish always --dir"
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.1"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9"
  },
  "build": {
    "appId": "com.yiku.electronupdaterrelease",
 
    "publish": [
      {
        "provider": "github",
        "owner": "Lsnxiaoxiong",
        "repo": "electron-updater-release"
      }
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  }
}